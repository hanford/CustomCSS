{
  "name": "Veneer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Use your own CSS across the web!",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon48.png",
    "128": "images/icon128.png"
    },
    "content_scripts": [{
      "matches": [
      "http://*/*",
      "https://*/*"
      ],
      "js": ["scripts/content.js"],
      "run_at": "document_end"
    }],
    "browser_action": {
      "default_popup": "index.html"
    },
    "background": {},
    "permissions": [
      "storage",
      "unlimitedStorage",
      "tabs",
      "webRequest",
      "webRequestBlocking"
  ],
  "default_locale": "en"
}
